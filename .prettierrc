{
    "singleAttributePerLine": true,
    "bracketSameLine": false,
    "jsxSingleQuote": false,
    "bracketSpacing": true,
    "singleQuote": false,
    "printWidth": 400,
    "useTabs": false,
    "tabWidth": 4,
    "semi": true,
    "endOfLine": "lf",
    "trailingComma": "all",
    "proseWrap": "preserve",
    "arrowParens": "always",
    "htmlWhitespaceSensitivity": "css",
    "embeddedLanguageFormatting": "auto",
    "overrides": [
        { "files": "*.json", "options": { "printWidth": 200 } },
        { "files": "*.md", "options": { "proseWrap": "always" } }
    ]
}
